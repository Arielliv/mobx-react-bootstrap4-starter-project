{
  "name": "typescript-react-mobx-boilerplate",
  "version": "1.0.0",
  "private": true,
  "description": "A frontend boilerplate with React, MobX and Typescript",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start-front": "cd client && webpack-dev-server -d --history-api-fallback --hot --inline --progress --colors --port 3000",
    "build-front": "cd client && webpack --progress --colors",
    "start-back": "cd server && nodemon --watch *.ts --exec ts-node index.ts",
    "watch-front": "nodemon -e js,ts,tsx --watch client --exec npm run build-front",
    "watch-back": "nodemon -e ts --watch server --exec npm run start-back",
    "watch-all": "npm run build-front && concurrently --kill-others \"npm run watch-front\" \"npm run watch-back\""
  },
  "license": "MIT",
  "devDependencies": {
    "@types/classnames": "^2.2.3",
    "@types/express": "^4.0.39",
    "@types/node": "^8.0.47",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^15.5.4",
    "@types/react-router": "^4.0.15",
    "@types/webpack": "^3.0.10",
    "awesome-typescript-loader": "^3.2.3",
    "axios": "^0.17.0",
    "concurrently": "^3.5.0",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.28.7",
    "error-handler": "^1.0.0",
    "express-session": "^1.15.6",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "method-override": "^2.3.10",
    "mobx-react-devtools": "^4.2.15",
    "morgan": "^1.9.0",
    "node-sass": "^4.5.3",
    "nodemon": "^1.12.1",
    "postcss": "^6.0.11",
    "postcss-browser-reporter": "^0.5.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^10.0.0",
    "postcss-loader": "^2.0.6",
